<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" href="/favicon.png">
    <title><%if(typeof pagedata!=='undefined' && pagedata.title){
      %><%- pagedata.title %><%
    } else if(locals.reactapp.settings.title){
      %><%- locals.reactapp.settings.title %><%
    } else if(locals.reactapp.settings.name){
      %><%- locals.reactapp.settings.name %><%
    } else{%>'Welcome'<%}%></title>
    <link href="/extensions/periodicjs.ext.reactapp/static/css/main.b57d567a.css" rel="stylesheet">
    <%- (locals.reactapp.settings.custom_css_stylesheet)?`<link href="${locals.reactapp.settings.custom_css_stylesheet}" rel="stylesheet">`:'' %>
    <% if(locals.reactapp.settings.application.additional_header){ %><%- include(`../../../../content/container/${locals.theme_name}/views/shared/reactapp/header`,{locals}) ;%><% } %>
  </head>
  <body>
    <div id="root"><%- (typeof body!=='undefined')?body:'' %></div>
    <script type="text/javascript">
      <%- locals.periodic.viewHelper.passObjToClient(locals.reactapp.settings,"__padmin") %>
    </script>
      <% if(locals.reactapp.settings.application.use_custom_elements){ %><%- include(`../../../../content/container/${locals.theme_name}/views/shared/reactapp/custom_elements`,{locals});%><% } %>
    
    <script type="text/javascript" src="/extensions/periodicjs.ext.reactapp/static/js/main.8aeacbca.js"></script>
    <% if(locals.reactapp.settings.application.additional_footer){ %><%- include(`../../../../content/container/${locals.theme_name}/views/shared/reactapp/footer`,{locals}) ;%><% } %>
  </body>
</html>